<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.chemik77.controllers.other.PersonalInfoController">
   <children>
      <GridPane prefHeight="100.0" prefWidth="500.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="25.0" />
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="25.0" />
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%personalinfo.last" />
            <Label text="%personalinfo.first" GridPane.rowIndex="1" />
            <Label text="%personalinfo.pesel" GridPane.rowIndex="2" />
            <Label text="%personalinfo.sex" GridPane.rowIndex="3" />
            <Label text="%personalinfo.birth" GridPane.rowIndex="4" />
            <Label text="%personalinfo.phone" GridPane.columnIndex="4" />
            <Label text="%personalinfo.email" GridPane.columnIndex="4" GridPane.rowIndex="1" />
            <Label text="%personalinfo.street" GridPane.columnIndex="4" GridPane.rowIndex="2" />
            <Label text="%personalinfo.house" GridPane.columnIndex="4" GridPane.rowIndex="3" />
            <Label text="%personalinfo.zip" GridPane.columnIndex="4" GridPane.rowIndex="4" />
            <Label text="%personalinfo.city" GridPane.columnIndex="4" GridPane.rowIndex="5" />
            <Label fx:id="lastLabel" GridPane.columnIndex="1" />
            <Label fx:id="firstLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="peselLabel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="sexLabel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="birthLabel" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label fx:id="phoneLabel" GridPane.columnIndex="5" />
            <Label fx:id="emailLabel" GridPane.columnIndex="5" GridPane.rowIndex="1" />
            <Label fx:id="streetLabel" GridPane.columnIndex="5" GridPane.rowIndex="2" />
            <Label fx:id="houseLabel" GridPane.columnIndex="5" GridPane.rowIndex="3" />
            <Label fx:id="zipLabel" GridPane.columnIndex="5" GridPane.rowIndex="4" />
            <Label fx:id="cityLabel" GridPane.columnIndex="5" GridPane.rowIndex="5" />
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </GridPane>
   </children>
</StackPane>
